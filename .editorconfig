root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
max_line_length = 80
ij_any_align_multiline_array_initializer_expression = true
ij_any_array_initializer_wrap = off
ij_any_array_initializer_new_line_after_left_brace = true
ij_any_array_initializer_right_brace_on_new_line = true
ij_any_spaces_within_braces = true
ij_any_spaces_within_brackets = true
